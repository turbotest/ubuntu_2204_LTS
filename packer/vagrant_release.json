{
	"builders": [
    {
      "type": "vagrant",
      "communicator": "ssh",
      "source_path": "{{ user `source_path` }}",
      "provider": "virtualbox",
      "add_force": true,
      "box_name": "{{ user `box_name` }}"
    }
	],
	"provisioners": [
    { "type": "shell", "script": "./test/all.sh" }
	],
  "post-processors": [
    {
      "type": "vagrant-cloud",
      "box_tag": "turbotest/ubuntu_2204_LTS",
      "access_token": "{{ user `cloud_token` }}",
      "version": "{{ user `version` }}-{{ timestamp }}",
      "architecture": "amd64"
    }
  ]
}